 @ApiStatus.Experimental
 public final class LifecycleEvents {
 
+    public static final LifecycleEventType.Monitor<BootstrapContext, RegistrarEvent.Reloadable<DummyResourceRegistrar>> DUMMY = bootstrap("dummy");
